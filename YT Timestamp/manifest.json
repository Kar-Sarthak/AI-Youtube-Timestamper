{
    "name": "YT Stamper",
    "description": "Load AI Generated timestamps for any youtube video",
    "version": "0.0.0.1",
    "manifest_version": 3,
      "host_permissions": [
    "http://localhost:5000/*"
    ],

    "content_scripts": [
        {
            "matches": [
                "https://*.youtube.com/*"                
            ],
            "js": [
                "content.js"
            ]
        }
    ],
      "web_accessible_resources": [
    {
      "resources": ["time.png"],
      "matches": ["*://*.youtube.com/*"]
    }
  ]
}